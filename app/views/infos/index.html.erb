<h1>Listing infos</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>About you</th>
      <th>Education</th>
      <th>Skills</th>
      <th>Hometown</th>
      <th>Email</th>
      <th>Phone no</th>
      <th>Dob</th>
      <th>Religion</th>
      <th>Gender</th>
      <th>Languages</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @infos.each do |info| %>
      <tr>
        <td><%= info.user_id %></td>
        <td><%= info.about_you %></td>
        <td><%= info.education %></td>
        <td><%= info.skills %></td>
        <td><%= info.hometown %></td>
        <td><%= info.email %></td>
        <td><%= info.phone_no %></td>
        <td><%= info.dob %></td>
        <td><%= info.religion %></td>
        <td><%= info.gender %></td>
        <td><%= info.languages %></td>
        <td><%= link_to 'Show', info %></td>
        <td><%= link_to 'Edit', edit_info_path(info) %></td>
        <td><%= link_to 'Destroy', info, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Info', new_info_path %>
